#!/usr/bin/env sh
set -e

cd /usr/local/bin
ftp https://raw.githubusercontent.com/openssh/openssh-portable/master/contrib/ssh-copy-id
chmod +x ssh-copy-id

cd /usr/local/man/man1
ftp https://raw.githubusercontent.com/openssh/openssh-portable/master/contrib/ssh-copy-id.1
makewhatis /usr/local/man