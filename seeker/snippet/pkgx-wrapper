#!/bin/bash

if type "pkgx" &> /dev/null; then
    echo "pkgx is already installed"
    eval "$(pkgx --shellcode)"
else
    echo "installing pkgx"
    eval "$(curl -Ssf https://pkgx.sh)"
fi

env +jq +yq

jq --version
yq --version
