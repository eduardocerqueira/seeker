#!/bin/bash
url="https://lite.duckduckgo.com/lite?q="
value="$*"
encoded_value=$(python3 -c "import urllib.parse; print(urllib.parse.quote('''$value''', safe=''))")
lynx ${url}${encoded_value} -trace