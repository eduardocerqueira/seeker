#!/bin/env bash

input="$(mktemp)"
output="$(mktemp)"

import "$input.png"
tesseract -l eng "$input.png" "$output" 2> /dev/null
cat "$output.txt"

rm -f "$input"
rm -f "$input.png"
rm -f "$output"
rm -f "$output.txt"
