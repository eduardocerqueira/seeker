#url: https://api.github.com/gists/159af1def5fe758c10b435bd9f2c5995
#owner: https://api.github.com/users/ajgappmark

#!/bin/bash
dconf reset -f /org/cinnamon/desktop/keybindings/ && \
dconf load /org/cinnamon/desktop/keybindings/ <<EOF
[/]
custom-list=['__dummy__', 'custom0']

[custom-keybindings/custom0]
binding=['<Super>Escape']
command='/usr/bin/gnome-terminal'
name='gnome-terminal'

[media-keys]
terminal=['<Primary><Alt>t']
EOF
